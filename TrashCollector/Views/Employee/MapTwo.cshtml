@model TrashCollector.Models.Customer
@{
    ViewBag.Title = "Index";
}

 @{ string address = String.Format("{0}+{1}+{2},+{3},+{4}", Model.HouseNumber, Model.StreetName, Model.StreetExt,Model.City, Model.State);}    

@{ 
    string url = String.Format("https://maps.googleapis.com/maps/api/geocode/json?address={0}&key=AIzaSyCcNDvH7rqMP5-OUD81MUXgV4vTGJzBkFE",address);
}
<!DOCTYPE html>
<html>
<head>
    <title>Map</title>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
    <style>
        /* Always set the map height explicitly to define the size of the div
        * element that contains the map. */
        #map {
            height: 500px;
        }
        /* Optional: Makes the sample page fill the window. */
        /*html, body {
             height: 50%;
             margin: 0;
             padding: 0;
         }*/
    </style>
</head>
<body>

    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            /**/
            var place = { lat:src["results"[1].geometry[0].lat], lng: src["results"[1].geometry[0].lng]}
        /**/
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: src["results"[1].geometry[0].lat], lng: src["results"[1].geometry[0].lng]},
            zoom: 15
        });
        var marker = new google.maps.Marker({ position: place, map: map });
    }
    </script>
    <script src=@url
            async defer></script>
</body>
</html>